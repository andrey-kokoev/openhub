  },
  "peerDependencies": {
    "nitropack": "^2.12.9"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "nitropack": "^2.12.9",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "publishConfig": {
    "access": "public"
  }
}