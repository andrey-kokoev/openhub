{
  "name": "@openhub2/runtime-nitro",
  "version": "0.0.1",
  "description": "OpenHub runtime integration for Nitro",
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
